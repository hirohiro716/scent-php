{
    
    "name": "hirohiro716/scent-php",
    
    "type": "library",
    
    "minimum-stability": "dev",
    
	"prefer-stable": true,
    
    "require": {
    	"php": ">=7.2",
        "phpmailer/phpmailer": "^6.0",
        "smarty/smarty": "^3.1",
        "phpgangsta/googleauthenticator": "*"
    },
    
    "autoload": {
        "psr-4": {
            "hirohiro716\\Scent\\": "class/",
            "hirohiro716\\Scent\\Filesystem\\": "class/filesystem/",
            "hirohiro716\\Scent\\Mail\\": "class/mail/",
            "hirohiro716\\Scent\\Database\\": "class/database/",
            "hirohiro716\\Scent\\Smarty\\": "class/smarty/"
        }
    }
    
}
