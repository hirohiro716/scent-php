{
    
    "name": "hirohiro716/scent-php",
    
    "type": "library",
    
    "prefer-stable": true,
    
    "require": {
    	"php": ">=7.2",
        "phpmailer/phpmailer": "^6.0",
        "smarty/smarty": "^3.1",
        "phpgangsta/googleauthenticator": "dev-master",
        "chillerlan/php-qrcode": "^3.1.0"
    },
    
    "autoload": {
        "psr-4": {
            "hirohiro716\\Scent\\": "class/",
            "hirohiro716\\Scent\\Auth\\": "class/auth/",
            "hirohiro716\\Scent\\Database\\": "class/database/",
            "hirohiro716\\Scent\\Filesystem\\": "class/filesystem/",
            "hirohiro716\\Scent\\Image\\": "class/image/",
            "hirohiro716\\Scent\\Mail\\": "class/mail/",
            "hirohiro716\\Scent\\Smarty\\": "class/smarty/",
            "hirohiro716\\Scent\\Validate\\": "class/validate/"
        }
    }
    
}
